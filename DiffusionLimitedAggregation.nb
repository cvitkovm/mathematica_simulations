Cell[BoxData[
 RowBox[{"While", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"Length", "[", "occupiedSites", "]"}], "<", "n"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rad", " ", "=", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Max", "[", 
        RowBox[{"Abs", "[", "occupiedSites", "]"}], "]"}], "+", "s"}], 
      ")"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"loc", " ", "=", " ", 
     RowBox[{"FixedPoint", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
