Cell["\<\
Putting all the code together into a module (remembering that we have to add \
2 to the output from this code to get fractal dimension):\
\>", "Text",
 CellChangeTimes->{{3.527909845935266*^9, 3.527909853735135*^9}, {
  3.5281338913678637`*^9, 3.528133894205555*^9}, {3.5281339264771357`*^9, 
  3.528133933420635*^9}}],

Cell[BoxData[{
 RowBox[{"Clear", "[", "r", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"FractalDimension", "[", "occupiedSites_List", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"occSiteDensity", ",", "fractalDataList", ",", "fractaldim"}], 
     "}"}], ",", 
    RowBox[{
     RowBox[{
      RowBox[{"occSiteDensity", "[", "t_Integer", "]"}], ":=", 
      RowBox[{
       RowBox[{"N", "[", 
        RowBox[{"Count", "[", 
         RowBox[{"occupiedSites", ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"x_", "?", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{"Abs", "[", "#", "]"}], "\[LessEqual]", "t"}], "&"}], 
              ")"}]}], ",", 
            RowBox[{"y_", "?", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{"Abs", "[", "#", "]"}], "\[LessEqual]", "t"}], "&"}], 
              ")"}]}]}], "}"}]}], "]"}], "]"}], "/", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", "t"}], "+", "1"}], ")"}], "^", "2"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"fractalDataList", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"2", "r"}], ",", 
          RowBox[{"occSiteDensity", "[", "r", "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"r", ",", 
          RowBox[{"Max", "[", 
           RowBox[{"Abs", "[", "occupiedSites", "]"}], "]"}]}], "}"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"fractalDim", " ", "=", " ", 
      RowBox[{"LinearModelFit", "[", 
       RowBox[{
        RowBox[{"N", "[", 
         RowBox[{"Log", "[", "fractalDataList", "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"1", ",", "z"}], "}"}], ",", "z"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", "\"\<{log[c], d} for our DLA is: \>\"", "]"}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
       RowBox[{"fractalDim", "[", "\"\<BestFitParameters\>\"", "]"}], "+", 
       "2"}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", "\"\<our errors in Log[c] and d are: \>\"", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"fractalDim", "[", "\"\<ParameterErrors\>\"", "]"}], "]"}], 
     ";"}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
