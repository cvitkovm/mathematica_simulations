Cell["\<\
Writing the code to get site density for a particular diameter:\
\>", "Text",
 CellChangeTimes->{{3.527909283056816*^9, 3.527909293310809*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"occSiteDensity", "[", "t_Integer", "]"}], ":=", 
  RowBox[{
   RowBox[{"N", "[", 
    RowBox[{"Count", "[", 
     RowBox[{"occupiedSites", ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x_", "?", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"Abs", "[", "#", "]"}], "\[LessEqual]", "t"}], "&"}], 
          ")"}]}], ",", 
        RowBox[{"y_", "?", 
         RowBox[{"(", 
