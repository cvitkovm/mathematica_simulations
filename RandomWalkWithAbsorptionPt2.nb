Cell["\<\
Creating a module varAveWalkTime[aStart,aEnd,b,numTrials] which will, for \
each value of a from aStart to aEnd, run numTrials random walks with \
absorption barriers at a and b (a<0, b>0) and return a list containing the \
average stopping time of the walk for all the values of a tried in the form \
{a,average stopping time}:\
\>", "Text",
 CellChangeTimes->{{3.5268695671298523`*^9, 3.5268696310935106`*^9}}],

Cell[BoxData[{
 RowBox[{"Clear", "[", "varAveWalkTime", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"varAveWalkTime", "[", 
   RowBox[{"aStart_", ",", "aEnd_", ",", "b_", ",", "numTrials_"}], "]"}], 
  " ", ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"outputList", ",", " ", "aCurrent"}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"aCurrent", " ", "=", " ", "aStart"}], ";", " ", 
     RowBox[{"outputList", " ", "=", " ", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"aCurrent", ">", "aEnd"}], ")"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"outputList", " ", "=", " ", 
          RowBox[{"Append", "[", 
           RowBox[{"outputList", ",", 
            RowBox[{"{", 
             RowBox[{"aCurrent", ",", 
              RowBox[{"aveWalkTime", "[", 
               RowBox[{"aCurrent", ",", "b", ",", "numTrials"}], "]"}]}], 
             "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"aCurrent", " ", "-=", " ", "1"}], ";"}], ")"}]}], "]"}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", "outputList"}]}], 
   "\[IndentingNewLine]", "]"}]}]}], "Input",
 CellChangeTimes->{{3.526869632810609*^9, 3.5268697446670065`*^9}}],

