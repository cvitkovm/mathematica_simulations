  3.526295311450157*^9}, {3.526295955800012*^9, 3.5262959598552437`*^9}, {
  3.5268679591618814`*^9, 3.5268679700595045`*^9}}],

Cell[BoxData[{
 RowBox[{"Clear", "[", "walkTime", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"walkTime", "[", 
   RowBox[{"a_", ",", "b_"}], "]"}], " ", ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"xPosition", ",", "t"}], "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"xPosition", "=", " ", "0"}], ";", " ", 
     RowBox[{"t", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"a", "<", "xPosition", " ", "<", "b"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"(", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"xPosition", "+=", "nextStep"}], ";", "\[IndentingNewLine]", 
         RowBox[{"t", "++"}], ";"}], "\[IndentingNewLine]", ")"}]}], "]"}], 
     ";", "\[IndentingNewLine]", "t"}]}], "\[IndentingNewLine]", 
   "]"}]}]}], "Input",
 CellChangeTimes->{{3.526294477148438*^9, 3.526294492445313*^9}, {
  3.526294533670671*^9, 3.5262945341847005`*^9}, {3.526294668682393*^9, 
